<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.121">
    <children>
        <CheckBox fx:id="securityEnabled" layoutX="14.0" layoutY="14.0" mnemonicParsing="false"
                  text="Aktywuj dodatkowe zabezpieczenia serwera"/>
        <Label layoutX="14.0" layoutY="48.0" text="Nazwa użytkownika:"/>
        <Label layoutX="100.0" layoutY="82.0" text="Hasło:"/>
        <TextField fx:id="userName" layoutX="144.0" layoutY="43.0"/>
        <PasswordField fx:id="password" layoutX="144.0" layoutY="77.0" promptText="(nie zmieniono)"/>
        <Label layoutX="14.0" layoutY="121.0"
               text="Serwer nie udostępnia hasła ze względów bezpieczeństwa. &#10;Aby zmienić hasło, wpisz nowe hasło do pola tekstowego i kliknij &quot;Zastosuj&quot; lub &quot;Ok&quot;."/>
        <Label fx:id="settingsChangedLabel" layoutX="14.0" layoutY="171.0"
               text="Ustawienia zostały zmienione. Zostaną zapisane po kliknięciu &quot;OK&quot; lub &quot;Zastosuj&quot;"
               textFill="#cd0e0e"/>
    </children>
</AnchorPane>
